encoding=utf8
[Actions]
requiredWinstVersion >= "4.11.5.2"

if CompareDotSeparatedNumbers("%WinstVersion%","<","4.12.0.13")
	if fileexists("%ScriptPath%\setup411.opsiscript")
		comment "Start old setupscript ..."
		sub "%ScriptPath%\setup411.opsiscript"
	endif
else
	if fileexists("%ScriptPath%\setup412.opsiscript")
		comment "Start new setupscript ..."
		sub "%ScriptPath%\setup412.opsiscript"
	endif
endif	
